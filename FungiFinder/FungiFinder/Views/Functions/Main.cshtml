@model FunctionMainVM

@{
    ViewData["Title"] = "Main";
}
@section scripts{
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.4.1.min.js"></script>
    <script src="~/js/Main.js"></script>
}

<h4>Fungi Finder</h4>
<hr />
<div id="result"></div>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Main" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input id="picture" asp-for="ImgToSearch" type="file" onchange="uploadFile('picture')"/>
            <div class="form-group">
                <input type="submit" value="Upload" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div id="resultHere"></div>
<div>
</div>



